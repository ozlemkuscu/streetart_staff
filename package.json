    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "browser-sync": "^2.18.7",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
