  "eslintConfig": {
    "env": {
      "es6": true,
      "node": false,
      "browser": true,
      "jquery": true
    },
    "fix": true,
    "globals": {
      "cot_app": true,
      "cot_form": true,
      "cot_section": true,
      "cot_row": true,
      "cot_login": true,
      "cot_grid": true,
      "CotModel": true,
      "CotForm": true,
      "dcsMultiTrack": true,
      "formatDate": true,
      "bootbox": true
