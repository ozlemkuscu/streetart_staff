    "extends": "eslint:recommended",
    "rules": {
      "no-undef": "warn",
      "no-console": "off"
    }
  },
  "coreConfig": {
    "isEmbedded": false,
    "includeFormValidation": true,
    "includeEditableSelect": true,
    "includePlaceholders": true,
    "includeMultiSelect": true,
    "includeOMS": false,
    "includeFullCalendar": false,
    "includeDatePicker": true,
    "includeLogin": true,
    "includeRangePicker": true,
    "includeMoment": true,
    "includeModeling": true,
    "includeWebtrends": true,
    "includeBootbox": true
